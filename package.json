{
  "name": "untitled-unmastered",
  "version": "2.0.0",
  "description": "Untitled Unmastered is template for React-Redux project with SCSS and FSD",
  "main": "index.tsx",
  "bin": {
    "create-tmtyl": "bin/create-tmtyl.mjs"
  },
  "scripts": {
    "start": "webpack serve --open --env mode=development",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "build:prodAnalyzer": "npm run build:prod -- --env analyzer=true",
    "build:mobile": "webpack --env mode=production --env platform=mobile",
    "build:desktop": "webpack --env mode=production --env platform=desktop",
    "lint": "eslint -c .eslintrc.json --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc",
    "test": "jest"
  },
  "keywords": [
    "template",
    "react",
    "reduxtoolkit",
    "scss",
    "fsd"
  ],
  "author": "Matthew Skomorokhov",
  "license": "MIT",
